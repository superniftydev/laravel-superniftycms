    .dz-message {

        text-align: center;
        margin: 3em 0;

        .dz-button {
            background: none;
            color: inherit;
            border: none;
            padding: 0;
            font: inherit;
            cursor: pointer;
            outline: inherit;

        }

    }

    .dz-default {
        margin: 0;
    }

}





.dz-preview {

    position: relative;
    display: flex;
    align-items: center;
    justify-content: center;
    color: #fff;

    transition: all 250ms ease-in-out;
    pointer-events: auto;
    cursor: pointer;
    width: 7rem;
    height: 7rem;
    border-radius: 7rem;
    margin: 0 1rem 1rem 0;

    .dz-image {
        box-shadow: 0 0 5px 2px rgba(0,0,150,.66);
    }


    &:after {
        position: absolute;
        top: .5em;
        left: 0;
        background-color: var(--color-gray-900);
        color: var(--color-white);
        font-size: .5rem;
        text-transform: uppercase;
        padding: .25rem .5rem;
        border-radius: .25rem;
        box-shadow: 1px 2px 3px rgba(0,0,0,.25);
        content: attr(data-title_size);
        white-space: nowrap;
        pointer-events: none;
        opacity: 0;
        transition: all 500ms ease-in-out;
        transition-delay: 750ms;
        z-index: 999;
    }

    &:hover {
        transform: scale(1.05);
        .dz-details {
            &:after {
                opacity: 1;
            }
        }

        .dz-image {
            box-shadow: 0 0 5px 2px rgba(0,0,150,.99);
        }

        &:after { opacity: 1; }

    }

    .dz-image:before,
    .dz-image:after,
    [data-dz-download],
    [data-dz-warn],
    .dz-size,
    .dz-details,
    .cm
    { display: none; }


    &.instructions {
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        text-align: center;
        background-color: var(--color-blue-100);
        border-color: var(--color-blue-100);
        color: var(--color-blue-400);
        padding: 1rem;
        font-size: .875rem;
        order: 99;
        pointer-events: none;
        user-select: none;

        &:before {
            font-family: 'Material Symbols Outlined';
            font-variation-settings: 'FILL' 1, 'wght' 600, 'GRAD' 200, 'opsz' 48;
            content: '\f1f0';
            font-size: 2rem;
        }

        .maxsize {
            font-weight: 900;
            font-size: 1.5rem;

        }
        .filetypes {
            font-size: .75rem;
        }

    }

    .dz-abbr {
        font-size: 2.5rem;
        top: 1rem;
        left: 0;
        justify-content: center;
        text-align: center;
    }

    .dz-abbr {
        position: absolute;
        top: .5rem;
        left: .75rem;
        display: flex;
        justify-content: flex-start;
        text-align: left;
        width: 100%;
        font-size: 3rem;
        line-height: 1.15;
        font-weight: 900;
        z-index: 9;
        pointer-events: none;
    }

    .dz-progress {

        opacity: 1;
        z-index: 1000;
        pointer-events: none;
        position: absolute;
        height: .5rem;
        top: calc(50% - .5rem);
        left: 15%;
        right: 15%;
        background: rgba(255,255,255,.1);
        border-radius: 2rem;
        overflow: hidden;
        transition: all 250ms ease-in-out;

        .dz-upload {
            background: #fff;
            display: block;
            position: relative;
            height: 100%;
            width: 0;
            transition: width 300ms ease-in-out;
            border-radius: 0;
        }

    }

    &.processing {

        pointer-events: none;

        &:before {
            position: absolute;
            top: 0;
            right: 0;
            width: 100%;
            height: 100%;
            z-index: 998;
            background-color: rgba(0,0,0,.75);
            color: var(--color-white);
            display: flex;
            flex-direction: column;
            text-align: center;
            justify-content: center;
            align-items: center;
            font-size: 2rem;
            content: '\e863';
            text-transform: uppercase;

            font-family: 'Material Symbols Outlined';
            font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 48;
            animation-name: spin;
            animation-duration: 2000ms;
            animation-iteration-count: infinite;
            animation-timing-function: linear;

        }

        @keyframes spin {
            from { transform:rotate(0deg); }
            to { transform:rotate(360deg); }
        }

        &:after {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            z-index: 999;
            color: var(--color-white);
            display: flex;
            flex-direction: column;
            text-align: center;
            justify-content: center;
            align-items: center;
            font-size: .5rem;
            content: 'processing';
            text-transform: uppercase;
            padding-top: 2rem;
        }

    }

    [data-dz-warn]{
        right: 0;
        &:before { content: '\e5cd'; }
        &:hover { color: var(--color-red-500); }
    }

    .dz-warn-w {

        position: absolute;
        top: 0;
        left: 0;
        z-index: 99;
        width: 100%;
        height: 100%;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        font-size: 1rem;
        font-weight: 700;
        color: var(--color-white);
        background-color: rgba(0,0,0,.7);
        text-transform: uppercase;
        transition: all 250ms ease-in-out;
        &:before { content: 'Delete?'; }
        opacity: 0;
        pointer-events: none;

        .w {
            display: flex;
            padding: .5rem 0;
        }

        [data-dz-remove],
        [data-dz-cancel] {
            display: flex;
            font-size: .75rem;
            font-style: normal;
            cursor: pointer!important;
            padding: .5rem 1rem;
            text-transform: uppercase;
            border-radius: .25rem;
            font-weight: 700;
            transition: all 250ms ease-in-out;
            &:first-child { margin-right: .5rem; }
            border: 1px solid var(--color-white);

        }

        [data-dz-cancel] {
            background-color: var(--color-gray-500);
            &:hover { background-color: var(--color-gray-600); }
            &:after { content: 'Cancel'; }
        }

        [data-dz-remove] {
            background-color: var(--color-red-500);
            &:hover { background-color: var(--color-red-600); }
            &:after { content: 'Delete!'; }
        }

    }

    [data-dz-download]{
        right: 2.5rem;
        &:before { content: '\f1e3'; } /* f1e3 | e8f4 */
        &:hover { color: var(--color-black); }
    }

    .dz-image{
        display: flex;
        justify-content: center;
        align-items: center;
        width: 100%;
        height: 100%;

        position: relative;
        z-index: 5;
        cursor: pointer!important;
        pointer-events: auto;
        border-radius: 7rem!important;
        overflow: hidden!important;



        background-size: 3rem;
        background-position: center;
        background-repeat: no-repeat;

        img {
            position: absolute;
            top: 0;
            left:0;
            width: 100%;
            height: 100%;
            display: flex;
            cursor: pointer!important;

            object-fit: cover;
        }

        /* display text for non-raster file types */
        &:after {
            display: flex;
            width: 100%;
            height: 100%;
            position: absolute;
            top: 0;
            left: 0;
            justify-content: center;
            align-items: center;
            z-index: 40;
            font-weight: 900;
            font-size: 2rem;
        }

    }

    .dz-details {
        position:absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        z-index: 15;
        display: flex;
        flex-direction: column;
        pointer-events: none;

        .dz-filename {

            position: absolute;
            bottom: 1.3rem;
            left: 0;
            overflow: hidden;
            display: block;
            line-height: 1;
            border-radius: .125rem;
            pointer-events: none;
            max-width: 90%;
            font-weight: 700;
            font-size: .875rem;
            padding: .125rem .5rem .25rem .5rem;
            font-family: 'Roboto Mono', monospace;
            color: var(--color-white);
            background-color: var(--color-black);
            margin: 0 auto 0 .5rem;
            user-select: none;
            opacity: 0;

        }

        .dz-video-id {
            display: none;
        }

        .dz-size {

            position: absolute;
            bottom: .5rem;
            left: .5rem;
            display: flex;
            align-items: center;
            font-family: 'Roboto Mono', monospace;
            background-color: var(--color-black);
            display: none;


            span:first-child {
                padding: .125rem .5rem;
                color: var(--color-white);
                font-size: .6rem;
                text-transform: uppercase;
                line-height: 1;
                font-weight: 700;
                z-index: 2;
                transition: all 250ms ease-in-out;
            }

            &:after {
                position: relative;
                display: flex;
                padding: .125rem .5rem .125rem .25rem;
                color: var(--color-white);
                font-size: .6rem;
                line-height: 1;
                text-transform: uppercase;
                font-weight: 700;
                z-index: 2;
                transition: all 250ms ease-in-out;
                content: attr(data-type);
            }
        }

        .dz-author {

            span.dz-created_by {
                &:before { content: ''; }
            }

            span.dz-created_at {
                &:before { content: ''; }
            }

        }

    }


    &[data-type='ppt'],
    &[data-type='pptx']{
        background-color: rgba(220,146,35,1);
        .dz-image:after { content: 'PPT'; }
    }

    &[data-type='doc'],
    &[data-type='docx']{
        background-color: rgba(66,117,188,1);
        .dz-image { img { opacity: 0; } &:after { content: 'DOC'; }}
    }

    &[data-type='txt']
    {
        background-color: var(--color-gray-500);
        .dz-image { img { opacity: 0; } &:after { content: 'TXT'; }}
    }

    &[data-type='glb']
    {
        background-color: var(--color-violet-500);
        .dz-image { img { opacity: 0; } &:after { content: 'GLB'; }}
    }

    &[data-type='vtt']
    {
        background-color: var(--color-fuchsia-500);
        .dz-image { img { opacity: 0; } &:after { content: 'VTT'; }}
    }

    &[data-type='json']{
        background-color: var(--color-gray-400);
        .dz-image { img { opacity: 0; } &:after { content: 'JSON'; }}
    }

    &[data-type='svg'] {
        background-color: rgba(0,0,0,.1);
        .dz-progress { display: none; }
    }

    &[data-type='pdf'] {
        background-color: rgba(199,50,55,1);
        .dz-image { img { opacity: 0; } &:after { content: 'PDF'; }}
    }

    &[data-type='xls'],
    &[data-type='xlsx'] {
        background-color: rgba(113,176,66,1);
        .dz-image { img { opacity: 0; } &:after { content: 'XLS'; }}
    }

    &[data-type='youtube'] {
        background-color: rgba(219,42,3,1);
    }

    &[data-type='vimeo'] {
        background-color: rgba(97,172,236,1);
    }

    &[data-type='gif'] .dz-image,
    &[data-type='png'] .dz-image,
    &[data-type='jpg'] .dz-image,
    &[data-type='jpeg'] .dz-image,
    &[data-type='webp'] .dz-image
    {
        background-color: var(--color-gray-100);
