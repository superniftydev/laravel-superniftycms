/* custom dropzone stuff */

#sn_existingMedia,
#sn_mediaDZTemplate,
.sn_mediaExisting,
#sn_selectedMediaIDs
{
    display: none;
}

.sn_mediaDZW {

    position: relative;
    display: flex;
    width: 100%;
    min-height: 7rem;
    transition: all 500ms ease-in-out;
    /* background-color: var(--color-blue-100); */
    /* box-shadow: inset 0 7px 9px -7px rgba(0,0,0,0.8); */
    /* box-shadow: inset 0 5px 5px -5px rgba(0,0,0,0.25); */
