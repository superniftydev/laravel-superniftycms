body#be-topic-edit {

    ul#sn_CMDZMedia {
        li {
            &.add-to-topic-field {
                &:before {
                    font-variation-settings: 'FILL' 1, 'wght' 400, 'GRAD' 200, 'opsz' 48;
                    content: '\e5cd'; /* content: '\e644'; */
                }
                &:after {
                    content: attr(data-remove);
                }
            }
        }
    }

}

body#be-media-all {

    ul#sn_CMDZMedia {
        li {
            &.add-to-topic-field { display: none; }
        }
    }

    &[data-topic_id]{
        ul#sn_CMDZMedia {
            li {
                &.add-to-topic-field { display: flex; }
            }
        }
    }

}


ul#sn_CMFiles {

    &[data-parent='config']{

        li[data-type='directory']{
            display: none;
        }
    }


}



