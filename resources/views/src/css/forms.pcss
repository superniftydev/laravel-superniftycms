                        transition: all 250ms ease-in-out;
                        color: var(--color-blue-50);
                        background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' fill='none' viewBox='0 0 20 20'%3e%3cpath stroke='%23ffffff' stroke-linecap='round' stroke-linejoin='round' stroke-width='1.5' d='M6 8l4 4 4-4'/%3e%3c/svg%3e");
                        cursor: pointer;
                        box-shadow: none;

                        &:hover {
                        }

                        &:focus {
                            outline: none;
                        }
                    }

                }

                &:nth-child(2) {
                    margin-right: 1rem;
                }

                &.init, &.mic, &.reset, &.done {
                    &:before {
                        position: absolute;
                        top: .25rem;
                        left: 0;
                        font-family: 'Material Symbols Outlined';
                        font-variation-settings: 'FILL' 1, 'wght' 600, 'GRAD' 200, 'opsz' 48;
                    }
                }

                &.init,
                &.mic,
                &.reset,
                &.done {
                    display: none;
                    width: 2rem;
                }

                &.init {
                    display: flex;
                    width: 1.5rem;
                    color: var(--color-gray-400);

                    &:before {
                        content: '\e029';
                    }

                    &:hover {
                        color: var(--color-gray-900);
                        transform: scale(1.15);
                    }
                }

                &.mic {

                    border-radius: 50%;

                    &:before {
                        content: '\e029';
                    }

                    &:after {
                        position: absolute;
                        top: .5rem;
                        left: 0;
                        width: 1rem;
                        height: 1rem;
                        border-radius: 50%;
                        animation: micListening 2s infinite;
                        content: '';
                        transition: all 250ms ease-in-out;
                        color: rgba(255, 255, 255, 0);
                    }

                }

                &.reset {
                    &:before {
                        content: '\f053';
                    }
                }

                &.done {
                    margin-right: 0;

                    &:before {
                        content: '\e876';
                    }
                }

            }

        }

        .final {

            display: flex;
            flex-wrap: wrap;
            counter-reset: inc;
            margin: 0;
            height: auto;
            max-height: 60vh;
            overflow-y:scroll;
            background-color: transparent;

            li {
                position: relative;
                display: flex;
                align-items: center;
                background-color: var(--color-blue-400);
                color: var(--color-white);
                border-radius: .25rem;
                padding: .5rem 1rem .5rem 2rem;
                margin: 0 2px 2px 0;
                cursor: grab;
                line-height: 1.25;

                &:before {
                    position: absolute;
                    top: 0;
                    left: 0;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    width: 2rem;
                    height: 100%;
                    font-size: 1.25rem;
                    line-height: 1;
                    font-family: 'Material Symbols Outlined';
                    font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 48;
                    content: '\e945';
                    color: var(--color-white);
                }

                /*

                &:before {
                    max-width: 0;
                    overflow: hidden;
                    counter-increment: inc;
                    content: counter(inc, decimal);
                    transition: all 250ms ease-in-out;
                    text-align:right;
                    background-color: var(--color-gray-100);
                }


                */


                span[contenteditable] {
                    cursor: text;
                    background-color: transparent;
                    display: flex;
                    align-items: center;
                    min-height: 1rem;
                    padding-bottom: .25rem !important;
                }


                i {
                    display: flex;
                    color: rgba(255, 255, 255, .5);
                    cursor: pointer;
                    font-style: normal;
                    transition: all 250ms ease-in-out;

                    &:before {
                        font-family: 'Material Symbols Outlined';
                        font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 22;
                        content: '\e5c9';
                    }

                    &:hover {
                        color: var(--color-white);

                        &:before {
                            content: '\e5c9';
                        }
                    }

                }

                .timestamp {
                    display: flex;
                    max-width: 0;
                    overflow: hidden;
                    color: var(--color-white);
                    padding: 0;
                    transition: all 250ms ease-in-out;
                }

                .content {
                    padding: 0 0 0 .5rem;
                }

            }

            color: var(--color-gray-900);

        }

        .interim {
            max-height: 0;
            overflow: hidden;
            display: flex;
            align-items: center;
            padding: 0 .25rem;
            transition: all 250ms ease-in-out;
            position: absolute;
            bottom: 0;
            left: 0;
            width: 100%;

            color: var(--color-white);
            font-weight: 400;
            background-color: var(--color-blue-300);
            margin-right: auto;
            font-size: 1rem;
            opacity: 0;

            &:before {
                font-family: 'Material Symbols Outlined';
                font-variation-settings: 'FILL' 0, 'wght' 700, 'GRAD' 200, 'opsz' 48;
                content: '\e5df';
                color: var(--color-white);
                font-size: 1.5rem;
            }

        }


        &.ready {


            border-bottom: none;

            .w {
                padding-bottom: 4rem;
            }


            ul.dictationTools {
                padding: .125rem .125rem .125rem 2rem;
                bottom: .5rem;


                &:before {
                    display: block;
                }

                li {

                    &:nth-child(1),
                    &:nth-child(2) {
                        max-width: 200px;
                        pointer-events: auto;
                        opacity: 1;
                    }

                }

                .init {
                    display: none;

                }

                .mic, .reset, .done {
                    display: flex;
                }
            }

            .interim {
                max-height: 100vh;
                opacity: 1;
                padding: .75rem 200px .75rem .75rem;
            }

            .final {
                padding: 1rem;
                margin-bottom: 3.5rem;
            }

        }


        &.listening {

            .final {
                margin-bottom: 3.5rem;
            }




            ul.dictationTools {

                li:nth-child(1),
                li:nth-child(2) {
                    opacity: 0;
                    pointer-events: none;
                    max-width: 0;
                }

                .mic {
                    overflow: visible;
                    color: var(--color-blue-500);

                    &:before {
                        font-variation-settings: 'FILL' 1, 'wght' 600, 'GRAD' 200, 'opsz' 48;
                        content: '\e029';
                    }

                    &:after {
                        color: var(--color-blue-500);
                    }

                }

            }


            label {
                /* color: var(--color-blue-500); */
            }


            .transcript {

                padding-bottom: 5rem;

                .interim {

                    padding: 1rem 200px 1rem .25rem;
                    max-height: 5rem;
                    color: var(--color-blue-500);
                    background-color: var(--color-blue-50);

                }
            }


        }

    }


    &:focus-within {


        label {
            /* color: var(--color-blue-400); */
        }

        .mic {
            /* color: var(--color-blue-400); */
        }
    }


