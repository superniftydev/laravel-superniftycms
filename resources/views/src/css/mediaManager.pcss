    }



    &.dz-error {

        @apply bg-red-500 text-white p-8;

        .dz-image,
        .dz-details,
        .dz-progress,
        [data-dz-download],
        [data-dz-warn]{
            display: none;
        }

        cursor: pointer!important;

        .dz-error-message {
            cursor: pointer!important;

            @apply flex flex-col justify-center w-full text-sm text-center;

            &:after {
                position: relative;
                padding: .25rem 1rem;
                border-radius: 1rem;
                background-color: var(--color-white);
                color: var(--color-red-500);
                content: 'OK';
                font-size: .75rem;
                margin: .5rem auto 0 auto;
                font-weight: 700;
            }

            span {
                cursor: pointer!important;
            }

        }

        .dz-error-mark {
            opacity: 1;
        }

    }


    .dz-success-mark, .dz-error-mark {
        pointer-events: none;
        opacity: 0;
        z-index: 500;
        position: absolute;
        display: block;
        top: 50%;
        left: 50%;
        margin-left: 0;
        margin-top: 0;
        background: #fff;
        border-radius: 50%;
    }


    /* while file is uploading */
    &.dz-processing,
    &.dz-processing-video {

        pointer-events: none;

        &:before {
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            z-index: 5;
            /* background-color: var(--color-blue-500); */
            color: #fff;
            font-size: 1rem;
            font-weight: 900;
            display: flex;
            justify-content: center;
            align-items: center;
            text-align: center;
            opacity: .75;
            padding-top: 1rem;
            transition: opacity 250ms ease-in-out;

        }

        .dz-image {

            background-color: var(--color-blue-500);

            img {
                opacity: .5;
            }
        }

        .dz-progress {
            opacity: 1;
        }

    }

    &.dz-processing {
        &:before {
            z-index:99;
            font-size: .875rem;
            font-weight: 400;
            opacity: 0;
            animation: fadeIn infinite alternate ease 1500ms;
            content: 'Working...';
        }
    }

    @keyframes fadeIn {
        0% { opacity: 0; }
        100% { opacity: 1; }
    }

    &.dz-processing-video {
        &:before {
            z-index:99;
            content: 'Processing Video...';
        }
    }

    &.dz-complete .dz-progress,
    &.dz-processing-video .dz-progress
    {
        opacity: 0;
    }

    &:not(.dz-processing) {
        .dz-progress {
            /* animation: pulse 6s ease infinite; */
        }
    }

    &.inuse [data-dz-warn]{
        pointer-events: none!important;
        &:before {
            font-family: 'Material Symbols Outlined';
            font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 48;
            content: '\e63f';
        }

    }

    &[data-type='gif'],
    &[data-type='png'],
    &[data-type='jpg'],
    &[data-type='jpeg'],
    &[data-type='webp'],
    &[data-type='asf'],
    &[data-type='wmv'],
    &[data-type='mkv'],
    &[data-type='mov'],
    &[data-type='mp4'],
    &[data-type='avi'],
    &[data-type='mpeg'],
    &[data-type='ogv'],
    &[data-type='webm']
    &[data-type='youtube'],
    &[data-type='vimeo']
    {
        .dz-details .dz-size {
            background-color: #000;
        }

    }

}

img[src="null"],
img[src="new"]
{ display: none!important; }

#imgDZExistingImages {
    display: none;
}


#mediaModal {

    position: fixed;
    top: 0;
    left: 0;
    width: 100vw;
    height: 100vh;
    z-index: 99999999;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 0 2rem;
    background-color: rgba(0,0,0,.8);
    opacity: 0;
    pointer-events: none;
    transition: opacity 250ms ease-in-out;

    .closeModal {

        &.x {
            position: absolute;
            top: 1rem;
            right: 1rem;
            z-index: 5;
            cursor:pointer;
            font-size: 2rem;
            transition: all 250ms ease-in-out;
            color: var(--color-black);

            &:before {
                font-family: 'Material Symbols Outlined';
                font-variation-settings: 'FILL' 0, 'wght' 600, 'GRAD' 200, 'opsz' 48;
                content: '\e5cd';
            }

            &:hover {
                transform: scale(1.25);
            }

        }

    }

    .contentW {

        position: relative;
        overflow-y: scroll;
        display: block;
        width: 100%;
        max-width: 96vw;

        .contentWI {

            position: relative;
            display: flex;
            flex-direction: column;
            width: 100%;
            max-height: 95vh;
            background-color: var(--color-white);

            @media (min-width: var(--screen-sm)) {}

            @media (min-width: var(--screen-md)) {
                flex-direction: row;
            }

            @media (min-width: var(--screen-lg)) {}

            #mediaW,
            .fieldsW {
                position: relative;
                display: flex;
                justify-content: center;
                background-color: var(--color-white);
                margin: 0 auto;
                width: 100%;
                max-width: 100%;

                @media (min-width: var(--screen-md)) {
                    max-width: 48vw;
                }

            }

            #mediaW {

                background-color: var(--color-gray-900);
                min-height: 50vh;

                #mediaTSW {
                    position: absolute;
                    top: 1rem;
                    left: .75rem;
                    display: flex;
                    font-size: .875rem;
                    font-weight: 700;
                    pointer-events: none;
                    color: var(--color-white);
                    background-color: var(--color-black);
                    padding: .25rem .75rem;
                    border-radius: .25rem;
                    line-height: 1;
                    z-index:9;
                    text-transform: uppercase;

                    #mediaSize {
                        margin-right: .33rem;

                        &:empty {
                            display: none;
                        }

                    }

                    #mediaType {

                    }

                }

                #userUploadedVideoViewer,
                #snDocumentImageViewer,
                #snVideoViewer,
                #outsideVendorVideoViewer {
                    display: none;
                }

                #snDocumentImageViewer {
                    position: relative;

                    width: 100%;
                    height: auto;
                    margin: 0;

                    max-width: 100%;
                    max-height: 50vh;

                    display: flex;
                    align-items: center;
                    justify-content: center;
                    background-color: var(--color-gray-900);
                    padding: 2vw;

                    @media (min-width: var(--screen-sm)) {}

                    @media (min-width: var(--screen-md)) {
                        width: 100%;
                    }

                    @media (min-width: var(--screen-lg)) {
                        width: 100%;
                    }

                    img {

                        max-width: 100%;
                        max-height:50vh;
                        object-fit: cover;
                    }
                }

                &[data-vendor_media_id='new'] img { display: none; }
                &[data-type='vimeo'] img { display: none; }
                &[data-type='youtube'] img { display: none; }

                video#snVideoViewer{
                    display: flex;
                    align-items: flex-start;
                }

                #outsideVendorVideoViewer {
                    position: relative;
                    height: 0;
                    overflow: hidden;
                    width: 100%;

                    iframe {
                        position: absolute;
                        top: 0;
                        left: 0;
                        width: 100%;
                        height: 100%;
                    }

                }

            }

            .fieldsW {
                display: flex;
                flex-direction: column;
                background-color: var(--color-white);
                padding: 1rem;

                @media (min-width: var(--screen-sm)) {}
                @media (min-width: var(--screen-md)) {}
                @media (min-width: var(--screen-lg)) {}

                .snfw {
                    box-shadow: none;
                    border: none;
                    padding: 1em 1em 0 1em;
                    font-size: .875rem;
                }

                .snfwi.mediacontenteditableW {

                    label {
                        color: var(--color-gray-300);
                        font-weight: 600;
                    }

                    .media-input[contenteditable] {
                        padding: 1em 0;
                        min-height: 3.5em;
                        color: var(--color-gray-500);
                    }

                }

                #outsideVendorVideoIDW {

                    display: none;

                    .snfwi.mediacontenteditableW {

                        label {}

                        #outsideVendorVideoID {

                            &:empty:before {
                                color: var(--color-gray-400);
                            }

                            &:focus:before {
                                background-color: var(--color-blue-50);
                                color: var(--color-blue-300);
                            }

                        }

                    }

                }

                .snfw {

                    &:focus-within {
                        background-color: var(--color-blue-50);

                        label {
                            color: var(--color-blue-400);
                        }

                        .media-input[contenteditable] {
                            color: var(--color-gray-800);
                        }

                    }

                }

                .closeModal {
                    @apply flex ml-auto py-4 px-12 text-white bg-gray-600 hover:bg-gray-900 cursor-pointer uppercase transition-all rounded;
                }

            }

        }

    }

}

body[data-media_modal_status='sni'],
body[data-media_modal_status='snv'],
body[data-media_modal_status='ovv'],
body[data-media_modal_status='uuv'],
body[data-media_modal_status='document']
{

    height: 100vh;
    overflow-y: hidden;

    #mediaModal {
        opacity: 1;
        pointer-events: auto;
    }


}

/* supernifty image */
body[data-media_modal_status='sni']{

    #mediaModal .contentW .contentWI #mediaW {

        @media (min-width: var(--screen-md)) {

        }

        #snDocumentImageViewer {
            display: flex;
            #documentSummary { display: none; }
        }

    }

}

/* user document */
body[data-media_modal_status='document']{

    #mediaModal .contentW .contentWI #mediaW {

        #snDocumentImageViewer {
            display: flex;
            img { display: none; }
            height: 100%;
            padding: 0;

            #documentSummary {
                position: relative;
                z-index: 1;
                display: flex;
                flex-direction: column;
                width: 100%;
                height: 100%;
                background-color: var(--color-gray-200);

                &:before {
                    position: absolute;
                    top: 0;
                    left: 0;
                    width: 100%;
                    height: 100%;
                    display: flex;
                    justify-content: center;
                    align-items: center;
                    color: #fff;
                    text-transform: uppercase;
                    font-size: 10vw;
                    font-weight: 900;

                    content: attr(data-type);
                }

                &[data-type='ppt'],
                &[data-type='pptx']{
                    background-color: rgba(220,146,35,1);
                }

                &[data-type='doc'],
                &[data-type='docx']{
                    background-color: rgba(66,117,188,1);
                }

                &[data-type='txt']{
                    background-color: var(--color-gray-500);
                }

                &[data-type='json']{
                    background-color: var(--color-gray-400);
                }

                &[data-type='svg'] {
                    background-color: rgba(197,45,124,1);
                }

                &[data-type='pdf'] {
                    background-color: rgba(199,50,55,1);
                }

                &[data-type='xls'],
                &[data-type='xlsx'] {
                    background-color: rgba(113,176,66,1);
                }

            }

        }

    }

}


/* supernifty video */
body[data-media_modal_status='snv'],
body[data-media_modal_status='uuv']
{

    #mediaModal .contentW .contentWI #mediaW {
        #snVideoViewer {
            display: block;
        }
        #snDocumentSummary {
            display: flex;
        }

    }

}



/* user uploaded video */
body[data-media_modal_status='uuv']{

    #mediaModal .contentW .contentWI {

        .fieldsW {
            #outsideVendorVideoIDW {
                display: none;
            }
        }

        #mediaW {
            display: flex;
            flex-direction: column;

            #mediaType {
            }

            @media (min-width: var(--screen-md)) {

            }

            #snDocumentImageViewer {
                display: none;
            }

            #userUploadedVideoViewer {

                position: relative;
                display: flex;
                border: 3px solid #333;
                height: 100%;
                width: 100%;

                #userUploadedVideoW {
                    position: relative;
                    display: flex;
                    justify-content: center;
                    height: 100%;
                    width: 100%;
                    background-color: var(--color-gray-900);
                    padding: 2vw;

                    video {}

                    img { display: none; }

                    &.pause img { opacity: 0; }
                    &.play img { opacity: 0; }


                }

            }

            &[data-type='svg'] {

                background-color: rgba(255,255,255,.1);
                background-size: cover;
                background-position: center;
                background-repeat: no-repeat;

            }

        }

        .fieldsW {
            padding-left: 1rem;
            width: 100%;

        }

    }

}

