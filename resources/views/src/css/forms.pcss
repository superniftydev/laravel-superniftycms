    ul.dictationTools,
    .transcript {
        display: none;
    }


    &.transcriptor {
        position: relative;

        ul.dictationTools {
            position: absolute;
            bottom: .25rem;
            right: 0;
            z-index: 22;
            display: flex;
            padding: .125rem;
            transition: all 250ms ease-in-out;


