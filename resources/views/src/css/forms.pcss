select {
    padding-right: 3rem !Important;
}

[contenteditable] span:not([role='presentation']) {
    opacity: .75;
}

.radio-toggle {
    @apply relative flex overflow-hidden bg-gray-100 rounded;
}

.radio-toggle input {
    position: absolute !important;
    clip: rect(0, 0, 0, 0);
    height: 1px;
    width: 1px;
    border: 0;
    overflow: hidden;
}

.radio-toggle label {
    @apply
    inline-flex
    items-center
    px-4
    py-2
    text-gray-400
    border
    border-transparent
    font-semibold
    uppercase
    tracking-widest
    hover:text-gray-700
    active:text-gray-900
    focus:outline-none
    focus:border-gray-900
    focus:ring
    focus:ring-gray-300
    disabled:opacity-25
    transition-all
    ;

    &:before {
        display: block;
        margin-right: .25rem;
        font-family: 'Material Symbols Outlined';
        font-variation-settings: 'FILL' 0, 'wght' 600, 'GRAD' 200, 'opsz' 48;
        content: '\e836';
        font-size: 150%;
        transition: all 250ms ease-in-out;
    }

    &:nth-child(2) {
        @apply rounded-l-md mr-0.5;
    }

    &:nth-child(4) {
        @apply mr-0.5;
    }

    &:last-child {
        @apply rounded-r-md;
    }

}

.radio-toggle label:hover {
    cursor: pointer;
}

.radio-toggle input:checked + label {
    @apply text-blue-500;

    &:before {
        font-size: 200%;
        content: '\e86c';
    }
}

#transcription {
    display: none;
}

select option:disabled {
    margin-top: 2rem;
}

@keyframes micListening {
    0% {
        transform: scale(0.95);
        box-shadow: 0 0 0 0 currentColor;
        opacity: .7;
    }

    70% {
        transform: scale(1);
        box-shadow: 0 0 0 10px currentColor;
        opacity: 0;
    }

    100% {
        transform: scale(0.95);
        box-shadow: 0 0 0 0 currentColor;
        opacity: 0;
    }

}


.buttonW {
    display: flex;
    width: 100%;
}


/* field set wraps */

.fsw {
    display: flex;
    flex-direction: column;
    margin-bottom: 1rem;

}

.fw {

    display: flex;
    flex-direction: column;
    transition: all 250ms ease-in-out;
    background-color: var(--color-gray-50);
    padding: .5rem;
    margin-bottom: 1px;
    border-radius: 0;
    border-bottom: 2px solid transparent;

    &:focus-within {
        background-color: var(--color-blue-50);
        border-color: var(--color-blue-500);

        label, input[type='text'] {
            color: var(--color-blue-500);
        }
    }

    &:first-child {
        border-radius: .25rem .25rem 0 0;
    }

    label {
        font-size: .75rem;
        color: var(--color-gray-800);
        font-weight: 400;
        margin-bottom: .25rem;
        cursor: pointer;

        strong {
            display: flex;
            font-weight: 700;
            margin-right: .25rem;
        }

        span {
            display: flex;
        }
    }

    input[type="text"] {
        font-size: 1rem;
        font-weight: 400;
        color: var(--color-gray-900);
        padding: 0;
        margin: 0;
        transition: all 250ms ease-in-out;
        background-color: transparent;

        &::placeholder {
            opacity: 0.5;
            color: var(--color-gray-500);
            font-size: .875rem;
        }

    }

    input[type="radio"],
    input[type="checkbox"] {
        opacity: 0;
        position: absolute;
    }

    input[type="radio"]:checked + label {
    }

    input[type="checkbox"]:checked + label {
        color: var(--color-gray-900) !important;
        background-color: transparent;
    }

    &.cbw {

        flex-direction: row;
        background-color: transparent;
        margin: 0;
        padding: 0;

        &.mt-2 {
            margin-bottom: .5rem;
        }

        &.mt-4 {
            margin-bottom: 1rem;
        }

        &.mb-2 {
            margin-bottom: .5rem;
        }

        &.mb-4 {
            margin-bottom: 1rem;
        }

        &:focus-within {
            background-color: transparent;
            border-color: transparent;

            label, input[type='text'] {
                color: var(--color-gray-800);


            }
        }

        label {
            display: flex;
            align-items: center;
            margin: 0;

            &:before {
                font-family: 'Material Symbols Outlined';
                font-variation-settings: 'FILL' 0, 'wght' 700, 'GRAD' 200, 'opsz' 48;
            }
        }

        &.toggle {
            label {
                &:before, &:after {
                    display: none;
                }
            }
        }

        i {
            display: block;
            position: relative;
            width: 32px;
            height: 18px;
            border-radius: 18px;
            border: 2px solid var(--color-gray-400);
            background-color: var(--color-gray-400);
            overflow: hidden;
            transition: all 100ms ease-in-out;
            margin-right: .5rem;

            &:before {
                display: block;
                position: absolute;
                top: 0;
                left: 0;
                width: 14px;
                height: 14px;
                border-radius: 14px;
                background-color: var(--color-white);
                box-shadow: 1px 2px 3px rgba(0, 0, 0, .2);
                content: '';
                transition: all 250ms ease-in-out;
            }

        }

    }

    input[type="radio"] + label:before {
        font-size: 150%;
        content: "\e836";
    }

    input[type="radio"]:checked + label:before {
        font-variation-settings: 'FILL' 1, 'wght' 600, 'GRAD' 200, 'opsz' 48;
        content: "\e86c";
    }

    input[type="checkbox"] + label:before {
        content: "\e836";
    }

    input[type="checkbox"]:checked + label:before {
        font-variation-settings: 'FILL' 1, 'wght' 600, 'GRAD' 200, 'opsz' 48;
        content: "\e86c";
    }

    input[type="checkbox"] + label {
    }

    input[type="checkbox"]:checked + label i {
        background-color: var(--color-blue-400);
        border-color: var(--color-blue-400);

        &:before {
            left: unset;
            right: 0;
        }
    }

    &:focus-within {
        background-color: var(--color-blue-50);
        border-bottom: 2px solid var(--color-blue-500);
    }

}


label.cbrb {
    display: flex;
    align-items: center;
    position: relative;
    cursor: pointer;
    line-height: 1.25;
    color: var(--color-gray-300);

    &:hover {
        color: var(--color-gray-500);
    }

    transition: all 250ms ease-in-out;

    span {
        display: flex;
        align-items: flex-end;
        font-size: .875rem;
        line-height: 1;

        strong {
            display: flex;
            margin-right: .5rem;
            font-size: 1rem;

        }
    }

    input {
        position: absolute;
        opacity: 0;
        cursor: pointer;
        height: 0;
        width: 0;
    }

    i {
        display: block;
        font-size: 1.25em;
        margin: .125em .25em -.125em 0;
        pointer-events: none;
        font-style: normal;

        &:before {
            font-family: 'Material Symbols Outlined';
            font-variation-settings: 'FILL' 0, 'wght' 700, 'GRAD' 200, 'opsz' 30;
        }
    }

    &.cb, &.rb {
        &:has(input:checked) {
            color: var(--color-gray-900);

            i {
                color: var(--color-gray-500);
            }
        }
    }

    &.cb {
        i:before {
            content: "\e835";
        }

        &:has(input:checked) i:before {
            font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 30;
            content: "\e834";
        }
    }

    &.rb {
        i:before {
            content: "\e836";
        }

        &:has(input:checked) i:before {
            font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 30;
            content: "\e86c";
        }
    }


}



