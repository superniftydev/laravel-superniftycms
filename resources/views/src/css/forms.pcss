#sn_globalLOF {
    display: none;
}



.labelSelect {
    display: flex;
    align-items: center;
    border: 1px solid var(--color-gray-300);
    border-radius: .25em;
    font-size: 1rem;

    label {
        padding: .25em 1em;
        text-transform: uppercase;
        color: var(--color-gray-400);
        font-size: .875em;
        pointer-events: none;
        user-select: none;
    }

    select {
        background-color: var(--color-white);
        border: none;
        border-radius: 0 .25em .25em 0;
        padding: .5em 1em;
        font-size: 1em;
        line-height: 1.5em;
        box-shadow: none;
        outline: none;
    }

}

.snfw {

    position: relative;
    display: flex;
    flex-direction: column;
    width: 100%;
    overflow: hidden;
    transition: all 250ms ease-in-out;
    background-color: var(--color-white);
    padding: 0;
    box-shadow: 1px 2px 3px rgba(0, 0, 0, .25);
    border-top: 1px solid var(--color-gray-400);
    pointer-events: auto;

    &.sortable-ghost {
        opacity: .5;
        background-color: var(--color-blue-50)!important;
        .settings {
            .n {
                background-color: var(--color-blue-500)!important;
            }
        }
    }

    &:hover {
        background-color: var(--color-gray-50);
    }


    &.active {
        background-color: var(--color-blue-50);
    }

    &:first-child {
    }

    &:last-child {
        margin-bottom: 1rem;
        border-radius: 0 0 .25rem .25rem;
    }

    .df {
        position: absolute;
        top: 1rem;
        left: 1rem;
        font-style: normal;
        width: 1rem;
        height: 1rem;
        border-radius: 2rem;
        z-index: 9999;
        cursor: pointer;
        color: var(--color-red-300);
        transition: all 250ms ease-in-out;

        &:before {
            font-family: 'Material Symbols Outlined';
            font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 48;
            content: '\e5c9';
        }

        &:hover {
            color: var(--color-red-500);
        }

    }

    .settings {

        position: relative;
        display: flex;
        align-items: center;

        /* sort handle */
        .h {
            position:absolute;
            top: 0;
            left: 0;
            width: 2rem;
            display: flex;
            justify-content: center;
            align-items: center;
            font-size: .875rem;
            user-select: none;
            cursor: pointer;
            z-index: 5;
            color: var(--color-white)!important;
            line-height: 1.875;

            &:before {
                font-family: 'Material Symbols Outlined';
                font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 48;
                content: '\e945';
            }

            &:hover {
                color: var(--color-gray-600);
            }
        }

        .attribute {
            position: relative;
            display: none;
            margin-right: 1px;
            font-size: .625rem;
            line-height: 2;
            color: var(--color-white);
            background-color: var(--color-blue-400);
            border-radius: 0;
            padding: 0 1rem 0 4rem;
            opacity: 0;
            pointer-events: none;
            transform: translateX(-10px);
            transition: all 250ms ease-in-out;
            margin-bottom: auto;

            &:before {
                position: absolute;
                top: 0;
                left: 0;
                bottom: 0;
                width: 4rem;
                display: flex;
                align-items: center;
                justify-content: flex-end;
                background-color: var(--color-blue-400);
                padding: 0 .5rem 0 0;
                border-radius: 0;
                font-size: .5rem;
                line-height: 1;
                text-transform: uppercase;
                color: var(--color-white);/*
                font-family: 'Material Symbols Outlined';
                font-variation-settings: 'FILL' 1, 'wght' 400, 'GRAD' 200, 'opsz' 22; */
            }

            &.layout { &:before { content: 'LAYOUT:'; width: 3.25rem; } padding: 0 0 0 3.25rem; }
            &.tag { &:before { content: 'TAG:'; width: 2rem; } padding: 0 0 0 2rem; }
            &.style { &:before { content: 'STYLE:'; width: 3rem; } padding: 0 0 0 3rem; }

            &.aft {
                padding: 0 1rem 0 3.5rem;
                &:before { content: 'ACCEPTS:'; width: 3.5rem; }
                &:empty:after {
                    content: 'file types';
                }
            }

            &.aft:not([contenteditable]) {
                user-select: none;
                pointer-events: none;
                margin-right: .5rem;

                ol {
                    display: flex;
                    align-items: center;

                    li {
                        margin-right: .125rem;
                    }
                }

            }

            select {

                border: none!important;
                outline:none!important;
                background-color: transparent;
                line-height: 2;
                padding: 0 1.5rem 0 0!important;
                color: var(--color-white)!important;
                font-size: .625rem;
                border-radius: 0;
                text-transform: uppercase;
                background-image: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' fill='none' viewBox='0 0 20 20'%3e%3cpath stroke='%23FFFFFF' stroke-linecap='round' stroke-linejoin='round' stroke-width='1.5' d='M6 8l4 4 4-4'/%3e%3c/svg%3e");

                &:focus {
                    outline:none!important;
                    box-shadow: none!important;
                }

                &::placeholder {
                    opacity: 0.5;
                    color: var(--color-gray-500);
                    font-size: inherit;
                }

            }

            &[contenteditable] {
                white-space: nowrap!important;
                text-transform: uppercase;
                &:focus {

                    &:before {

                    }
                }
            }

            &.n {
                display: flex;
                min-width: 10px;
                font-weight: 700;
                padding: 0 2rem 0 1.75rem;
                font-size: .825rem;
                background-color: var(--color-gray-400);
                color: var(--color-white);
                border-radius: 0 0 .25rem 0;
                opacity: 1;
                pointer-events: auto;
                transform: none;
                margin-bottom: auto;

                &:before {
                    display: none;
                }
                overflow: hidden; /* so the layout doesn't shift */
            }

            &:focus {
                background-color: var(--color-blue-600);
                &:before {
                    color: var(--color-blue-400);
                    background-color: var(--color-blue-100);
                }

            }

        }

        i.x {

            font-style: normal;
            transition: all 250ms ease-in-out;
            position: relative;
            color: var(--color-gray-200);
            cursor: pointer;
            font-size: 1rem;
            width: 1rem;
            margin: .25rem .5rem .25rem auto;

            &:before {
                content: 'delete?';
                font-size: .6rem;
                font-weight: 700;
                text-transform: uppercase;
                position: absolute;
                left: -4rem;
                top: 3px;
                padding: .125rem .5rem;
                background-color: var(--color-red-500);
                color: var(--color-white);
                border-radius: .125rem;
                display: none;
            }

            &:after {
                font-family: 'Material Symbols Outlined';
                font-variation-settings: 'FILL' 0, 'wght' 700, 'GRAD' 200, 'opsz' 48;
                content: '\e872';
            }

            &:hover {
                color: var(--color-red-300);
            }

        }



    }

    &.active,
    &:focus-within {

        border-top: 1px solid var(--color-blue-400);
        background-color: var(--color-blue-50)!important;
        color: var(--color-blue-900)!important;

        .settings {

            .h {
                color: var(--color-white)!important;

                &:hover {
                    color: var(--color-white)!important;
                }
            }

            .attribute {
                opacity: 1;
                pointer-events: auto;
                transform: translateX(0);

                &.n {
                    background-color: var(--color-blue-400);

                    &:focus {
                        background-color: var(--color-red-400);
                    }
                }
            }

            i.x {
                color: var(--color-blue-400)!important;

                &:hover {
                    color: var(--color-red-500)!important;
                }
            }

        }

        &.transcriptor ul.dictationTools li.init {
            color: var(--color-blue-400);

            &:hover {
                color: var(--color-blue-500);
            }

        }

    }


    /* media adjustments */
    &[data-type='media']{

        background-color: var(--color-blue-100);
        border-color: var(--color-blue-400);

        &:hover,
        &.dz-drag-hover
        {
            background-color: var(--color-blue-200);

            .settings {
                i.x {
                    color: var(--color-red-500);
                }
            }

            &:before {
                border-color: var(--color-red-300);
                color: var(--color-white);
            }
        }

        .settings {

            .h {
                color: var(--color-blue-300);
            }


            .attribute {

                display: flex;
                background-color: var(--color-blue-400)!important;
                color: var(--color-white)!important;
                transform: none;
                opacity: 1;
                pointer-events: auto;

                &:before {
                    background-color: var(--color-blue-400)!important;
                    color: var(--color-white)!important;
                    transition: all 250ms ease-in-out;
                }

                &:empty {
                    &:after {
                        opacity: .33;
                        color: var(--color-blue-400)!important;
                    }
                }

                &.n {
                    display: flex;
                    background-color: var(--color-blue-400)!important;
                    color: var(--color-white)!important;

                    &:focus {
                        background-color: var(--color-red-400)!important;
                    }

                }

                select {
                    color: var(--color-white)!important;
                }

                &:focus {
                    background-color: var(--color-blue-500)!important;
                    &:before {
                        background-color: var(--color-blue-500)!important;
                    }
                }

            }

            div:last-child {
                i.x {
                    color: var(--color-blue-300);

                    &:hover {
                        color: var(--color-red-500);

                    }
                }
            }

        }

    }

    &.warn {

        background-color: var(--color-red-100) !important;
        color: var(--color-red-500) !important;

        .settings,
        .settings div,
        .tools .init {
            background-color: var(--color-red-100) !important;
            color: var(--color-red-500) !important;
        }

        .settings .attribute.aft,
        .settings .attribute.style,
        .settings .attribute.tag,
        .settings .attribute.layout {
            display: none;
        }

        .settings div:last-child i.x {
            pointer-events: auto;

            &:before {
                display: flex;
            }

            &:after {
                color: var(--color-red-500);
                font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 48;
                content: '\e92b';
            }


        }



        .settings {
            pointer-events:none!important;
        }
        .settings .attribute.n[contenteditable] {
            pointer-events:none!important;
        }

        .settings .h {
            background-color: transparent!important;
            &:before { color: var(--color-red-300); }
        }

        .settings .attribute.n[contenteditable] {
            color: var(--color-white)!important;
            background-color: var(--color-red-500)!important;
            &:hover { pointer-events: none; }
        }

        .aft {
            ol {
                li {
                    background-color: var(--color-red-400)!important;
                }
            }
        }

        .aft[contenteditable] {
            border-color: var(--color-red-300)!important;
            background-color: rgba(255,255,255,.2);
            color: var(--color-red-400);
            &:hover { color: var(--color-red-500); }
            &:focus {
                color: var(--color-red-500)!important;
                border-color: var(--color-red-300)!important;
            }
        }

        select {
            border: 1px solid var(--color-red-300)!important;
            color: var(--color-red-400)!important;
        }


        .sn_mediaDZW {
            /* background-color: var(--color-red-300)!important; */

        }
        .ubtn {
            opacity: 0!important;
            pointer-events: none;
        }

        ul.dictationTools li.init {
            color: var(--color-red-400)!important;

            &:hover {
                color: var(--color-red-500)!important;
            }

        }

    }


    &#titleSNFW {

        padding-top: 3rem;
        border-radius: .25rem .25rem 0 0;
        border-top: none!important;

        &:before, &:after {
            position: absolute;
            left: 0;
            top: 0;
            color: var(--color-gray-400);
            pointer-events: none;
            user-select: none;
        }

        &:before {
            font-family: 'Material Symbols Outlined';
            font-variation-settings: 'FILL' 1, 'wght' 700, 'GRAD' 200, 'opsz' 48;
            content: '\e873';
            font-size: 1rem;
            z-index: 2;
            padding: .25rem .5rem;
        }

        &:after {
            content: 'page title';
            font-size: .675rem;
            text-transform: uppercase;
            padding: .55rem 1rem .5rem 1.75rem;
            background-color: var(--color-gray-50);
            border-radius: 0 0 .25rem 0;
            z-index: 1;
        }


        &:focus-within {

            &:before {
                color: var(--color-blue-400);
            }

            &:after {
                color: var(--color-blue-400);
                background-color: var(--color-blue-100);
            }

            #gotourl {
                color: var(--color-blue-300);
                &:hover {
                    color: var(--color-blue-600);
                }

            }

        }


    }



