    &.dz-started {}

    &:hover {
        /* background-color: var(--color-blue-200); */
        &:before {

        }
    }

    &.empty {

        &:before {
            justify-content: center;
            align-items: center;
        }

    }

    &:not(.empty) {
        .tools {
            label {}
        }
    }

    &.dz-clickable {
        cursor: pointer;

        * {
            cursor: default;
        }
        .dz-message {
            &, * {
                cursor: pointer;
            }
        }
    }

